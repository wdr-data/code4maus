{
  "name": "scratch-gui",
  "version": "0.1.0",
  "description": "GraphicaL User Interface for creating and running Scratch 3.0 projects",
  "main": "./dist/scratch-gui.js",
  "scripts": {
    "build": "webpack --colors --bail",
    "build:backend:netlify": "netlify-lambda build src/backend -c webpack.netlify-functions.js",
    "build:netlify": "yarn assets:netlify && yarn build && yarn build:backend:netlify && node scripts/build-netlify-redirects.js",
    "clean": "rimraf ./build && mkdirp build && rimraf ./dist && mkdirp dist",
    "deploy:build": "serverless package",
    "deploy:apply": "serverless deploy -p .serverless",
    "deploy:frontend": "./scripts/deploy-frontend.sh",
    "deploy": "yarn deploy:build && yarn deploy:apply && yarn deploy:frontend",
    "deploy:code4maus": "aws --region us-east-1 cloudformation deploy --stack-name code4maus-redirect --template-file scripts/deployment-code4maus.yml",
    "cache-assets": "AWS_REGION=eu-central-1 ASSET_BUCKET=hackingstudio-code4maus-projects-prod node scripts/extract-assets.js",
    "i18n:src": "babel src > tmp.js && rimraf tmp.js && build-i18n-src ./translations/messages/ ./translations/",
    "import-sprites": "node scripts/import-sprites.js",
    "assets:download": "aws s3 sync s3://code4maus-assets assets",
    "assets:netlify": "pip install awscli && AWS_ACCESS_KEY_ID=$DEPLOY_AWS_ACCESS_KEY_ID AWS_SECRET_ACCESS_KEY=$DEPLOY_AWS_SECRET_ACCESS_KEY npm run assets:download",
    "assets:upload": "aws s3 sync --exclude .gitkeep --exclude .DS_Store assets s3://code4maus-assets",
    "start": "webpack-dev-server",
    "start:backend": "serverless offline",
    "test": "npm run test:lint && npm run test:unit && npm run build && npm run test:integration",
    "test:integration": "jest --runInBand test[\\\\/]integration",
    "test:lint": "eslint . --ext .js,.jsx",
    "test:unit": "jest test[\\\\/]unit",
    "test:smoke": "jest --runInBand test[\\\\/]smoke",
    "watch": "webpack --progress --colors --watch"
  },
  "author": "Massachusetts Institute of Technology",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/LLK/scratch-gui#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LLK/scratch-gui.git"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@wdr-data/scratch-blocks": "^0.1.2-ids",
    "@wdr-data/scratch-vm": "0.2.0-minimal-3",
    "arraybuffer-loader": "^1.0.3",
    "autoprefixer": "^8.1.0",
    "aws-sdk": "^2.226.1",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
